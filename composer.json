{
    "name": "maba/wordpress-symfony-migration-demo",
    "type": "project",
    "authors": [
        {
            "name": "Marius Balƒçytis",
            "email": "marius.balcytis@gmail.com"
        }
    ],
    "require": {
        "th3mouk/yahoo-weather-api": "^1.0",
        "symfony/dependency-injection": "^4.0",
        "symfony/yaml": "^4.0",
        "symfony/config": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "platform": {
        "php": "7.1"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    }
}
